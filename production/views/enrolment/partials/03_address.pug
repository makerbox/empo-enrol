fieldset(name="Permanent Street Address")
  legend.gamma.font-header Permanent Street Address
  .fieldRow
    .fieldGroup.row
      .groupLabel.third
        label.label(for='sbuildingName') Building Name
      .groupFields.two-third
        .fieldInput
          input#sbuildingName.input(type='text', name='sbuildingName')

  .fieldRow
    .fieldGroup.row
      .groupLabel.third
        label.label(for='sunitNo') Unit Details
      .groupFields.two-third
        .fieldInput
          input#sunitNo.input(type='text', name='sunitNo')

  .fieldRow
    .fieldGroup.row
      .groupLabel.third
        label.label(for='sstreetNo') Street
      .groupFields.two-third
        .fieldInput.third
          label.label(for='sstreetNo') Street Number
          input#sstreetNo.input(type='text', name='sstreetNo', autocomplete='off')
        .fieldInput.two-third
          label.label(for='sstreetName') Street Name
          input#sstreetName.input(type='text', name='sstreetName', autocomplete='off')
  .fieldRow
    .fieldGroup.row
      .groupLabel.third
        label.label(for='sPOBox') PO Box Details
      .groupFields.two-third
        .fieldInput
          input.input.hassublabel#sPOBox(type='text', name='sPOBox')
          label.sublabel e.g. PO Box 88, RMB 123 or CARE PO
  .fieldRow
    .fieldGroup.row
      .groupLabel.third
        label.label(for='scity') City/Suburb
      .groupFields.two-third
        .fieldInput
          input.input#scity(type='text', name='scity')
  .fieldRow
    .fieldGroup.row
      .groupLabel.third
        label.label(for='sstate') State
      .groupFields.two-third
        .fieldInput
          select#sstate.select.input(name='sstate', autocomplete='state')
            option(value='',selected,disabled)  -- select --
            option(value='NSW') NSW
            option(value='VIC') VIC
            option(value='QLD') QLD
            option(value='SA') SA
            option(value='WA') WA
            option(value='TAS') TAS
            option(value='NT') NT
            option(value='ACT') ACT
            option(value='OTH') OTH (AUS)
            option(value='OVS') OVS (Overseas)


  .fieldRow
    .fieldGroup.row
      .groupLabel.third
        label.label(for='spostcode') Postcode
      .groupFields.two-third
        .fieldInput
          input.input#spostcode(type='tel', name='spostcode' size='4', maxlength='4', autocomplete='postal-code')

  .fieldRow
    .fieldGroup.row
      .groupLabel.third
        label.label(for='scountry') Country
